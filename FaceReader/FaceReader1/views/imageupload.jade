extends layout

block content
  h1= title

  form#formUploadImage(name="addimage",method="post",action="/addimage",enctype="multipart/form-data")
        input#inputImageName(type="text", placeholder="imagename", name="imagename")
        input#inputImage(type="file", placeholder="userimage", name="userimage",multiple='multiple')
        button#btnSubmit(type="submit") submit